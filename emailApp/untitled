	 		 	time_difference = sgt_now() - cms.last_sent
	 	time_difference_in_minutes = time_difference / timedelta(minutes=1)

	 	if (cms.active ==True and time_difference_in_minutes>=30):
	 		email=EmailMessage(json_obj[expectedAttr["TITLE"]], json_obj[expectedAttr["BODY"]], to=[json_obj[expectedAttr["TO"]]])
			email.send()
			response = JsonResponse({
		 		expectedAttr["ACTIVE"] : cms.active,
		 		"success" : True,
		 		})